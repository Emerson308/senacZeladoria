{
  "expo": {
    "name": "senacZeladoria",
    "slug": "senacZeladoria",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"

    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app acesses your photos to let you share them with your friends.",
          "cameraPermission": "The app acesses your camera to let you share them with your friends."
        }
      ]
    ]
  },
  "permissions": [
    "CAMERA",
    "MEDIA_LIBRARY"
  ],
  "plugins": [
    "expo-MEDIA_LIBRARY"
  ]
}
